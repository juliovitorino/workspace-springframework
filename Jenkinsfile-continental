pipeline {
    agent any
    stages {
        stage("Build Backend") {
            steps {
                dir("labs-modularization/death-agreement") {
                    sh 'mvn clean package -DskipTests'
                }
            }
        }
        stage("Run Unit Tests") {
            steps {
                dir("labs-modularization/death-agreement") {
                    sh 'mvn test'
                }
            }
        }
    }
}
